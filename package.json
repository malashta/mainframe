{
    "name": "mainframe",
    "version": "0.19.0",
    "description": "AngularJS Authentication on popular services",
    "main": "index.js",
    "scripts": {
        "dev": "better-npm-run start-dev",
        "dev-watch": "better-npm-run start-dev-watch"
    },
    "betterScripts": {
        "start-dev": {
            "command": "webpack --progress --colors",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "start-dev-watch": {
            "command": "webpack --progress --colors --watch",
            "env": {
                "NODE_ENV": "development"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/malashta/mainframe.git"
    },
    "author": {
        "name": "Malashta Sergey",
        "email": "malashta@gmail.com",
        "url": "http://lexek.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/malashta/mainframe/issues"
    },
    "homepage": "https://github.com/malashta/mainframe#readme",
    "devDependencies": {
        "babel-core": "^6.21.0",
        "babel-preset-es2015": "^6.18.0",
        "better-npm-run": "0.0.14",
        "clean-webpack-plugin": "^0.1.15",
        "eslint": "^3.13.1",
        "eslint-friendly-formatter": "^2.0.7",
        "lodash": "^4.17.4",
        "path": "^0.12.7",
        "webpack": "^1.14.0"
    }
}
