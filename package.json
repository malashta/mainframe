{
    "name": "mainframe",
    "version": "0.19.0",
    "description": "A starter mainframe on authenticate for AngularJS",
    "main": "index.js",
    "scripts": {
        "postinstall": "bower install",
        "dev": "better-npm-run start-dev",
        "dev-watch": "better-npm-run start-dev-watch",
        "start": "http-server -p 3000 ./client -d"
    },
    "betterScripts": {
        "start-dev": {
            "command": "webpack --progress --colors",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "start-dev-watch": {
            "command": "webpack --progress --colors --watch",
            "env": {
                "NODE_ENV": "development"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/malashta/mainframe.git"
    },
    "author": {
        "name": "Malashta Sergey",
        "email": "malashta@gmail.com",
        "url": "http://lexek.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/malashta/mainframe/issues"
    },
    "homepage": "https://github.com/malashta/mainframe#readme",
    "devDependencies": {
        "angular": "^1.6.1",
        "babel-core": "^6.21.0",
        "babel-loader": "^6.2.10",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-angular": "^6.0.15",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-stage-0": "^6.16.0",
        "better-npm-run": "0.0.14",
        "bower": "^1.8.0",
        "clean-webpack-plugin": "^0.1.15",
        "eslint": "^3.13.1",
        "eslint-friendly-formatter": "^2.0.7",
        "html-webpack-plugin": "^2.26.0",
        "http-server": "^0.9.0",
        "jade": "^1.11.0",
        "jade-loader": "^0.8.0",
        "jquery": "^3.1.1",
        "lodash": "^4.17.4",
        "path": "^0.12.7",
        "raw-loader": "^0.5.1",
        "strip-loader": "^0.1.2",
        "webpack": "^1.14.0"
    }
}
